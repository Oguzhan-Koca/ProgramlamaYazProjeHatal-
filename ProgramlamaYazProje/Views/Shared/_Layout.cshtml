                                <a href="#"><span>Sub Menu</span> <i class="bi bi-chevron-down dropdown-indicator"></i></a>
                                <ul>
                                    <li><a href="#">Sub Menu 1</a></li>
                                    <li><a href="#">Sub Menu 2</a></li>
                                    <li><a href="#">Sub Menu 3</a></li>
                                </ul>
                            </li>*@
                        </ul>
                    </li>
                    @*<li><a href="services.html">Services</a></li>*@
                    
                    <li><a asp-controller="Contact" asp-action="Index" class="active">İletişim</a></li>
                    <li>
                        <a asp-area="" asp-controller="Account" asp-action="Login"><i class="fa-solid fa-right-to-bracket me-2"></i>Login</a>
                    </li>                                                     
                    <li >
                         <a asp-area="" asp-controller="Account" asp-action="Register"><i class="fa-solid fa-user me-2"></i>Register</a>
                    </li>
